<div class='col-sm-9'>
  <div class='panel panel-primary' id='items-panel'>
    <div class='panel-heading'>{{controller.feed.title}} - {{controller.feed.description}}</div>
    <div class='panel-body'>
      <table class='table table-condensed table-striped'><tbody>
      {{#each model as |item|}}
        {{#link-to 'feeds.feed.feeditems.feeditem' feed.id item.id tagName='tr'}}
          {{#link-to 'feeds.feed.feeditems.feeditem' feed.id item.id tagName='td'}}
            {{link-to item.title 'feeds.feed.feeditems.feeditem' feed.id item.id}}
          {{/link-to}}
        {{/link-to}}
      {{/each}}
      </tbody></table>
    </div>
  </div>
</div>
{{! Feed Item }}
{{outlet}}
